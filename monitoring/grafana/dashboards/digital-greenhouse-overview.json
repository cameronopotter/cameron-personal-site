{
  "dashboard": {
    "id": null,
    "title": "Digital Greenhouse - System Overview",
    "tags": ["digital-greenhouse", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Score",
        "type": "stat",
        "targets": [
          {
            "expr": "digital_greenhouse_health_score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 85
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "digital_greenhouse_active_users_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(digital_greenhouse_http_requests_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(digital_greenhouse_http_requests_total{status=~\"4..|5..\"}[5m]) / rate(digital_greenhouse_http_requests_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Response Time Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(digital_greenhouse_http_request_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.90, rate(digital_greenhouse_http_request_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "90th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(digital_greenhouse_http_request_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(digital_greenhouse_http_request_duration_seconds_bucket[5m]))",
            "refId": "D",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=\"digital-greenhouse-backend\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "container_memory_usage_bytes{name=\"digital-greenhouse-backend\"} / container_spec_memory_limit_bytes{name=\"digital-greenhouse-backend\"} * 100",
            "refId": "B",
            "legendFormat": "Memory Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(digital_greenhouse_db_query_duration_seconds_sum[5m]) / rate(digital_greenhouse_db_query_duration_seconds_count[5m])",
            "refId": "A",
            "legendFormat": "Avg Query Time"
          },
          {
            "expr": "digital_greenhouse_db_connections_active",
            "refId": "B",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Cache Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(digital_greenhouse_cache_hits_total[5m]) / (rate(digital_greenhouse_cache_hits_total[5m]) + rate(digital_greenhouse_cache_misses_total[5m])) * 100",
            "refId": "A",
            "legendFormat": "Cache Hit Rate %"
          },
          {
            "expr": "digital_greenhouse_cache_memory_usage_bytes / 1024 / 1024",
            "refId": "B",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "Business Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "digital_greenhouse_projects_viewed_total",
            "refId": "A",
            "legendFormat": "Projects Viewed"
          },
          {
            "expr": "digital_greenhouse_skills_explored_total",
            "refId": "B",
            "legendFormat": "Skills Explored"
          },
          {
            "expr": "digital_greenhouse_session_duration_seconds",
            "refId": "C",
            "legendFormat": "Avg Session Duration"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 20
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1
  }
}