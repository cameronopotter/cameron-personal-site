{
  "dashboard": {
    "id": null,
    "title": "Digital Greenhouse - Performance Dashboard",
    "tags": ["digital-greenhouse", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Frontend Performance",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Page Load Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "digital_greenhouse_page_load_duration_seconds{quantile=\"0.5\"}",
            "refId": "A",
            "legendFormat": "Median Load Time"
          },
          {
            "expr": "digital_greenhouse_page_load_duration_seconds{quantile=\"0.95\"}",
            "refId": "B",
            "legendFormat": "95th Percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 1
        }
      },
      {
        "id": 3,
        "title": "Core Web Vitals",
        "type": "timeseries",
        "targets": [
          {
            "expr": "digital_greenhouse_first_contentful_paint_seconds",
            "refId": "A",
            "legendFormat": "First Contentful Paint"
          },
          {
            "expr": "digital_greenhouse_largest_contentful_paint_seconds",
            "refId": "B",
            "legendFormat": "Largest Contentful Paint"
          },
          {
            "expr": "digital_greenhouse_cumulative_layout_shift",
            "refId": "C",
            "legendFormat": "Cumulative Layout Shift"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 1
        }
      },
      {
        "id": 4,
        "title": "3D Rendering Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "digital_greenhouse_fps_current",
            "refId": "A",
            "legendFormat": "Current FPS"
          },
          {
            "expr": "digital_greenhouse_render_time_milliseconds",
            "refId": "B",
            "legendFormat": "Render Time (ms)"
          },
          {
            "expr": "digital_greenhouse_draw_calls_total",
            "refId": "C",
            "legendFormat": "Draw Calls"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 9
        }
      },
      {
        "id": 5,
        "title": "Memory Usage (Frontend)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "digital_greenhouse_js_heap_used_bytes / 1024 / 1024",
            "refId": "A",
            "legendFormat": "JS Heap Used (MB)"
          },
          {
            "expr": "digital_greenhouse_texture_memory_bytes / 1024 / 1024",
            "refId": "B",
            "legendFormat": "Texture Memory (MB)"
          },
          {
            "expr": "digital_greenhouse_geometry_memory_bytes / 1024 / 1024",
            "refId": "C",
            "legendFormat": "Geometry Memory (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 9
        }
      },
      {
        "id": 6,
        "title": "Backend Performance",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 7,
        "title": "API Response Times by Endpoint",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(digital_greenhouse_http_request_duration_seconds_bucket[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 18
        }
      },
      {
        "id": 8,
        "title": "Database Query Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(digital_greenhouse_db_query_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(digital_greenhouse_db_query_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(digital_greenhouse_db_query_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 9,
        "title": "WebSocket Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "digital_greenhouse_websocket_connections_active",
            "refId": "A",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "rate(digital_greenhouse_websocket_messages_sent_total[5m])",
            "refId": "B",
            "legendFormat": "Messages Sent/sec"
          },
          {
            "expr": "rate(digital_greenhouse_websocket_messages_received_total[5m])",
            "refId": "C",
            "legendFormat": "Messages Received/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 26
        }
      },
      {
        "id": 10,
        "title": "Performance Alerts",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 34
        }
      },
      {
        "id": 11,
        "title": "Performance Issues",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertname=~\".*Performance.*\"}",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 35
        }
      },
      {
        "id": 12,
        "title": "Lighthouse Scores",
        "type": "timeseries",
        "targets": [
          {
            "expr": "digital_greenhouse_lighthouse_performance_score",
            "refId": "A",
            "legendFormat": "Performance Score"
          },
          {
            "expr": "digital_greenhouse_lighthouse_accessibility_score",
            "refId": "B",
            "legendFormat": "Accessibility Score"
          },
          {
            "expr": "digital_greenhouse_lighthouse_best_practices_score",
            "refId": "C",
            "legendFormat": "Best Practices Score"
          },
          {
            "expr": "digital_greenhouse_lighthouse_seo_score",
            "refId": "D",
            "legendFormat": "SEO Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 43
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1
  }
}