{
  "name": "digital-greenhouse-frontend",
  "version": "1.0.0",
  "description": "Interactive 3D Digital Garden Frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:accessibility": "playwright test --grep=\"@accessibility\"",
    "test:performance": "playwright test --grep=\"@performance\"",
    "test:visual": "playwright test --grep=\"@visual\"",
    "playwright:install": "playwright install",
    "accessibility:audit": "npm run accessibility:axe && npm run accessibility:pa11y",
    "accessibility:axe": "axe http://localhost:3000 --save ./accessibility-results/axe-results.json --dir ./accessibility-results",
    "accessibility:pa11y": "pa11y http://localhost:3000 --reporter json > ./accessibility-results/pa11y-results.json"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@react-three/fiber": "^8.15.12",
    "@react-three/drei": "^9.92.7",
    "@react-three/postprocessing": "^2.15.11",
    "three": "^0.160.0",
    "@types/three": "^0.160.0",
    "framer-motion": "^10.16.16",
    "zustand": "^4.4.7",
    "@mui/material": "^5.15.4",
    "@mui/icons-material": "^5.15.4",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "d3": "^7.8.5",
    "@types/d3": "^7.4.3",
    "leva": "^0.9.35",
    "react-spring": "^9.7.3",
    "use-gesture": "^10.3.0",
    "react-intersection-observer": "^9.5.3",
    "react-hotkeys-hook": "^4.4.1",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "simplex-noise": "^4.0.1",
    "postprocessing": "^6.34.1",
    "maath": "^0.10.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "vitest": "^1.1.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/user-event": "^14.5.1",
    "@vitest/ui": "^1.1.0",
    "jsdom": "^23.0.1",
    "@playwright/test": "^1.40.1",
    "@axe-core/playwright": "^4.8.5",
    "lighthouse": "^11.4.0",
    "percy-playwright": "^1.0.6",
    "pa11y": "^8.0.0",
    "axe-core": "^4.8.3",
    "@axe-core/cli": "^4.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}